                  <Font size="19.0" />
               </font>
            </Label>
            <Button fx:id="btnLoginFirstTer" layoutX="117.0" layoutY="121.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="156.0" text="login" />
            <Button fx:id="btnViewFirstParkingSpot" layoutX="387.0" layoutY="121.0" mnemonicParsing="false" prefHeight="46.0" prefWidth="156.0" text="view empty parking spots" />
            <Button fx:id="btnFirstBack" layoutX="121.0" layoutY="307.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="72.0" text="back" />
